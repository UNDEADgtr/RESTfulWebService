{   "apiVersion": "0.2",
    "swaggerVersion": "1.1",
    "basePath": "http://petstore.swagger.wordnik.com/api",
    "resourcePath": "/user",
    "apis": [
        {
            "path": "/user/{username}",
            "description": "Operations about user",
            "operations": [
                {
                    "httpMethod": "GET",
                    "summary": "Get user by user name",
                    "responseClass": "User",
                    "nickname": "getUserByName",
                    "parameters": [
                        {
                            "name": "username",
                            "description": "The name that needs to be fetched. Use user1 for testing.",
                            "paramType": "path",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        }
                    ],
                    "errorResponses": [
                        {
                            "code": 400,
                            "reason": "Invalid username supplied"
                        },
                        {
                            "code": 404,
                            "reason": "User not found"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/users/",
            "description": "Operations about user",
            "operations": [
                {
                    "httpMethod": "GET",
                    "summary": "Get user by user name",
                    "responseClass": "User",
                    "nickname": "getUserByParameters",
                    "parameters": [
                        {
                            "name": "access",
                            "defaultValue": "true",
                            "description": "User has access or no",
                            "required": false,
                            "allowableValues": {
                                "values": ["false", "true"],
                                "valueType": "LIST"
                            },
                            "dataType": "string",
                            "paramType": "query",
                            "allowMultiple": false
                        }
                    ],
                    "errorResponses": [
                        {
                            "code": 400,
                            "reason": "Invalid username supplied"
                        },
                        {
                            "code": 404,
                            "reason": "User not found"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/user/login",
            "description": "Operations about user",
            "operations": [
                {
                    "httpMethod": "GET",
                    "summary": "Logs user into the system",
                    "responseClass": "string",
                    "nickname": "loginUser",
                    "parameters": [
                        {
                            "name": "username",
                            "description": "The user name for login",
                            "paramType": "query",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        },
                        {
                            "name": "password",
                            "description": "The password for login in clear text",
                            "paramType": "query",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "string"
                        }
                    ],
                    "errorResponses": [
                        {
                            "code": 400,
                            "reason": "Invalid username and password combination"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/user",
            "description": "Operations about user",
            "operations": [
                {
                    "httpMethod": "POST",
                    "summary": "Create user",
                    "notes": "This can only be done by the logged in user.",
                    "responseClass": "void",
                    "nickname": "createUser",
                    "parameters": [
                        {
                            "description": "Created user object",
                            "paramType": "body",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "User"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/user/createWithList",
            "description": "Operations about user",
            "operations": [
                {
                    "httpMethod": "POST",
                    "summary": "Creates list of users with given list input",
                    "responseClass": "void",
                    "nickname": "createUsersWithListInput",
                    "parameters": [
                        {
                            "description": "List of user object",
                            "paramType": "body",
                            "required": true,
                            "allowMultiple": false,
                            "dataType": "List[User]"
                        }
                    ]
                }
            ]
        },
        {
            "path": "/user/{permalink}",
            "description": "Operations about user",
            "operations": [
                {
                    "parameters": [
                        {
                            "name": "permalink",
                            "description": "permalink of user to update",
                            "required": true,
                            "dataType": "string",
                            "paramType": "path",
                            "allowMultiple": false
                        },
                        {
                            "description": "Updated user",
                            "required": false,
                            "dataType": "User",
                            "paramType": "body",
                            "allowMultiple": false
                        }
                    ],
                    "summary": "Updates an existing User",
                    "httpMethod": "PUT",
                    "errorResponses": [
                        {
                            "reason": "Invalid ID supplied",
                            "code": 400
                        },
                        {
                            "reason": "Not Authorized to update User",
                            "code": 403
                        },
                        {
                            "reason": "User not found",
                            "code": 404
                        }
                    ],
                    "nickname": "updateUser",
                    "responseClass": "ok"
                },
                {
                    "parameters": [
                        {
                            "name": "permalink",
                            "description": "ID of User to delete",
                            "required": true,
                            "dataType": "string",
                            "paramType": "path",
                            "allowMultiple": false
                        }
                    ],
                    "summary": "Deletes an existing User",
                    "httpMethod": "DELETE",
                    "errorResponses": [
                        {
                            "reason": "Invalid ID supplied",
                            "code": 400
                        },
                        {
                            "reason": "Not Authorized to delete User",
                            "code": 403
                        },
                        {
                            "reason": "User not found",
                            "code": 404
                        }
                    ],
                    "nickname": "deleteUser",
                    "responseClass": "ok"
                }
            ]
        }
    ], "models": {
    "User": {
        "id": "User",
        "properties": {
            "id": {
                "type": "long"
            },
            "lastName": {
                "type": "string"
            },
            "phone": {
                "type": "string"
            },
            "username": {
                "type": "string"
            },
            "email": {
                "type": "string"
            },
            "userStatus": {
                "allowableValues": {
                    "valueType": "LIST",
                    "values": ["1-registered", "2-active", "3-closed"],
                    "valueType": "LIST"
                },
                "description": "User Status",
                "type": "int"
            },
            "firstName": {
                "type": "string"
            },
            "password": {
                "type": "string"
            }
        }
    }
}}